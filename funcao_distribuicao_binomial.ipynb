    "\n",
    "def Probabilidade(n,w,p):\n",
    "    resultado = s.comb(n,w)*p**w*(1-p)**(n-w)\n",
    "\n",
    "    return resultado\n",
    "\n",
    "\n",
    "Probabilidade(15,14,0.4)\n",
    "\n"
   ]
